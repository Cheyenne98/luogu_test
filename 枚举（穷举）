在所有候选答案中搜索正确的
1.可确定候选答案数量

#include<bits/stdc++.h>  
using namespace std;
int a,b,c,d,e;
int main()
{

for(int a=1;a<=9;a++)
{
	for(b=0;b<=9;b++)
	{
		for(int c=0;c<=9;c++)
		{
			for(int d=0;d<=9;d++)
			{
				for(int e=0;e<=9;e++)
				{
					int cheng=a*10000+b*1000+c*100+d*10+e;
					int bei=a;
					int ans=e*100000+e*10000+e*1000+e*100+e*10+e;
					if(cheng*bei==ans)
					{
						cout<<" "<<cheng<<endl;
						cout<<"x"<<"    "<<a<<endl;
						cout<<ans<<endl;
					}
				}
			}
		}
	}
}
return 0;
}
